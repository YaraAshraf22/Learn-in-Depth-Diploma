/*C program to store info (name, roll, marks) of a student using structure*/

#include "stdio.h"

struct Sdata{
	char name[50];
	int roll;
	int marks;
};

struct Sdata read(void)
{
	struct Sdata student;
	printf("Enter name: ");
	fflush(stdin); fflush(stdout);
	scanf("%s",student.name);


	return student;
}
int main ()
{
	struct Sdata student;

	return 0;

}
