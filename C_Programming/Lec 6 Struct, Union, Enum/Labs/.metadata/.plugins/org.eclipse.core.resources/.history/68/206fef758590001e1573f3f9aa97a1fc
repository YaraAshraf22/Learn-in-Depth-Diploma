struct Scomplex Readcomplex(char name[])
{
	struct Scomplex c;
	printf("Enter complex number %s, ex (5, -3):",name);
	fflush(stdin); fflush(stdout);
	scanf("%d, %d", &c.m_R, &c.m_I);
	return c;
}

