#ifndef KEYPAD_H_
#define KEYPAD_H_

#include "STM32F103x6.h"
#include "STM32_F103C6_GPIO_Driver.h"

#define KEYPAD_GPIO GPIOB

//Rows PD0-3
#define R0 0
#define R1 1
#define R2 3
#define R3 4
//Columns PD4-7
#define C0 5
#define C1 6
#define C2 7
#define C3 8

void KEYPAD_INIT();
char KEYPAD_GET_CHAR();

#endif /* KEYPAD_H_ */
